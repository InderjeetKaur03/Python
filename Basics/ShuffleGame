# Functions

from random import shuffle

## Simple Game

def shuffle_list(mylist):
 shuffle(mylist)
 return mylist

def player_guess():
  guess=''
  guess = input("Pick a number : 0,1,2")
  return int(guess)

my_list = ['','','','','o','','']
mixed_list = shuffle_list(my_list)
guess = player_guess()

